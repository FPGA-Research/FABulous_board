    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 33)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.6)
    )
    (layer_rule GND
      (active off)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.6)
    )
    (layer_rule PWR
      (active off)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.6)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 101.6)
    (clearance 76.2)
    (clearance 50.8 (type smd_to_turn_gap))
    (clearance 19.2 (type smd))
    (clearance 76.2 (type "kicad_default"))
    (clearance 152.4 (type 6mil))
  )
  (padstack "Via[0-3]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle GND 800.0 0.0 0.0)
    )
    (shape
      (circle PWR 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-3]_800:400_um" "Via[0-3]_800:400_um" default
  )
  (via 
    "Via[0-3]_800:400_um-kicad_default" "Via[0-3]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-3]_800:400_um-6mil" "Via[0-3]_800:400_um" 6mil
  )
  (via_rule
    default "Via[0-3]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-3]_800:400_um-kicad_default"
  )
  (via_rule
    6mil "Via[0-3]_800:400_um-6mil"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
